<html lang="en" data-theme="dark">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{{title}}</title>
    <!-- Advanced Meta Tags for API Playground -->
    <meta name="description" content="Nebura API Playground is an interactive environment for testing, exploring, and documenting API endpoints. Easily send requests, view responses, and experiment with authentication, headers, and parameters." />
    <meta name="keywords" content="Nebura, API Playground, Testing, Documentation, Swagger, Postman, Hoppscotch, Endpoints, Requests, Responses, JWT, Headers, Parameters, Developer Tools, Interactive, REST, JSON, Code Samples" />
    <meta name="author" content="Hiroshi025" />
    <meta name="application-name" content="Nebura API Playground" />
    <meta name="subject" content="API Testing and Documentation" />
    <meta name="copyright" content="Hiroshi025" />
    <meta name="robots" content="index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1" />
    <meta name="theme-color" content="#222831" />
    <meta name="url" content="https://github.com/Hiroshi025/Nebura-AI" />
    <link rel="canonical" href="https://github.com/Hiroshi025/Nebura-AI" />
    <!-- Open Graph Tags -->
    <meta property="og:title" content="Nebura API Playground" />
    <meta property="og:description" content="Experiment, test, and document Nebura API endpoints interactively. Send requests, view responses, and explore authentication and headers." />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://github.com/Hiroshi025/Nebura-AI" />
    <meta property="og:image" content="https://i.pinimg.com/1200x/58/08/35/5808355cb825f18671975d00cbe10870.jpg" />
    <meta property="og:image:alt" content="Nebura Playground Logo" />
    <meta property="og:site_name" content="Nebura API Playground" />
    <meta property="og:locale" content="en_US" />
    <meta property="og:updated_time" content="2024-06-01T00:00:00+00:00" />
    <!-- Twitter Card Tags -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content="Nebura API Playground" />
    <meta name="twitter:description" content="Interactive API playground for Nebura. Test endpoints, send requests, and view responses in real time." />
    <meta name="twitter:image" content="https://i.pinimg.com/1200x/58/08/35/5808355cb825f18671975d00cbe10870.jpg" />
    <meta name="twitter:image:alt" content="Nebura Playground Logo" />
    <meta name="twitter:site" content="@hiroshi025" />
    <meta name="twitter:creator" content="@hiroshi025" />
    <!-- Advanced SEO -->
    <meta name="rating" content="safe for kids" />
    <meta name="distribution" content="global" />
    <meta name="revisit-after" content="7 days" />
    <meta name="expires" content="never" />
    <meta name="category" content="Technology, API, Playground, Developer Tools" />
    <link
      href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap"
      rel="stylesheet" />
    <link rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" />
    <link rel="stylesheet" href="/css/playground.css" />
    <link rel="shortcut icon" href="/assets/icono.jpg" type="image/x-icon" />
    <link rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.8.0/styles/github-dark.min.css" />
  </head>
  <body>
    <div class="sidebar">
      <a href="https://nestjs.com/" target="_blank" class="sidebar-icon"
        title="NestJS"><i class="fa-brands fa-nestjs"></i></a>
      <div class="sidebar-tooltip">NestJS</div>
      <a href="https://jwt.io/" target="_blank" class="sidebar-icon"
        title="JWT"><i class="fa-solid fa-key"></i></a>
      <div class="sidebar-tooltip">JWT</div>
      <a href="https://swagger.io/tools/swagger-ui/" target="_blank"
        class="sidebar-icon" title="Swagger"><i
          class="fa-solid fa-book"></i></a>
      <div class="sidebar-tooltip">Swagger</div>
      <a href="https://www.postman.com/" target="_blank" class="sidebar-icon"
        title="Postman"><i
          class="fa-solid fa-flask"></i></a>
      <div class="sidebar-tooltip">Postman</div>
      <a href="https://hoppscotch.io/" target="_blank" class="sidebar-icon"
        title="Hoppscotch"><i
          class="fa-solid fa-bolt"></i></a>
      <div class="sidebar-tooltip">Hoppscotch</div>
      <a href="https://apidocjs.com/" target="_blank" class="sidebar-icon"
        title="apiDoc"><i
          class="fa-solid fa-file-code"></i></a>
      <div class="sidebar-tooltip">apiDoc</div>
      <a href="https://www.base64decode.org/" target="_blank"
        class="sidebar-icon" title="Base64"><i
          class="fa-solid fa-code"></i></a>
      <div class="sidebar-tooltip">Base64</div>
      <a href="https://jwt.io/" target="_blank" class="sidebar-icon"
        title="JWT Debugger"><i class="fa-solid fa-bug"></i></a>
      <div class="sidebar-tooltip">JWT Debug</div>
    </div>
    <canvas id="animated-bg"></canvas>
    <div class="container">
      <div class="main-content">
        <header>
          <div class="logo">
            <i class="fas fa-key"></i>
            <span id="titleText">{{subtitle}}</span>
          </div>
          <div class="controls">
            <div class="controls-row">
              <button class="btn btn-outline history-btn" id="historyBtn">
                <i class="fas fa-history"></i>
                <span id="historyText">History</span>
                <div class="history-dropdown" id="historyDropdown"></div>
              </button>
              <select class="language-selector" id="languageSelector">
                <option value="en">English</option>
                <option value="es">Español</option>
                <option value="fr">Français</option>
              </select>
              <button class="theme-toggle" id="themeToggle">
                <i class="fas fa-moon"></i>
              </button>
            </div>
            <div class="controls-row">
              <button class="btn btn-outline btn-sm" id="clearResponseBtn">
                <i class="fas fa-trash-alt"></i>
                <span id="clearBtnText">Clear</span>
              </button>
              <button class="btn btn-primary btn-sm" id="sendRequestBtn">
                <i class="fas fa-paper-plane"></i>
                <span id="sendBtnText">Send Request</span>
              </button>
              <button class="btn btn-outline btn-sm" id="exportHistoryBtn"
                title="Exportar historial">
                <i class="fas fa-file-export"></i>
              </button>
              <button class="btn btn-outline btn-sm" id="importHistoryBtn"
                title="Importar historial">
                <i class="fas fa-file-import"></i>
              </button>
            </div>
          </div>
        </header>

        <div class="card" id="documentationCard">
          <div class="card-title">
            <span id="documentationTitle">API Documentation</span>
          </div>
          <div id="documentationContent" class="documentation-content">
            <div class="doc-section">
              <div class="doc-section-title" id="endpointTitle">Endpoint</div>
              <span class="doc-endpoint" id="endpointPath">{{endpoint}}</span>
              <span class="doc-method" id="endpointMethod">GET</span>
            </div>
            <div class="doc-section">
              <div class="doc-section-title"
                id="parametersTitle">Parameters</div>
              <ul class="doc-param-list">
                <li><b id="paramKey">key</b>
                  <span
                    style="color:var(--primary-light);font-size:0.95em;">(string,
                    <span id="requiredText">required</span>)</span>:
                  <span id="paramKeyDesc">License key</span></li>
                <li><b id="paramIdentifier">identifier</b>
                  <span
                    style="color:var(--primary-light);font-size:0.95em;">(string,
                    <span id="requiredText2">required</span>)</span>:
                  <span id="paramIdentifierDesc">Unique identifier</span></li>
              </ul>
            </div>
            <div class="doc-section">
              <div class="doc-section-title" id="exampleRequestTitle">Example
                Request</div>
              <div class="doc-example" id="exampleRequestText">
                GET
                <span style="color:var(--primary-light)">{{url}}</span>
              </div>
            </div>
            <div class="doc-section">
              <span class="doc-tip" id="tipText">Tip: Puedes usar herramientas
                como Postman, Hoppscotch o Swagger para probar este
                endpoint.</span>
            </div>
          </div>
        </div>

        <div class="card">
          <div class="card-title">
            <span id="apiEndpointText">API Endpoint</span>
            <div class="tooltip">
              <button class="copy-btn" id="copyUrlBtn">
                <i class="far fa-copy"></i>
              </button>
              <span class="tooltiptext" id="copyTooltipText">Copy to
                clipboard</span>
            </div>
          </div>
          <div class="url-display">
            <span class="method" id="methodText">GET</span>
            <span id="apiUrl">Loading base URL...</span>
          </div>
        </div>

        <div class="card">
          <div class="card-title">
            <span id="requestParamsText">Request Parameters</span>
          </div>
          <div class="form-group">
            <label for="licenseKey" class="form-label"
              id="licenseKeyLabel">License Key</label>
            <input type="text" id="licenseKey" class="form-control"
              placeholder="Enter license key" />
            <div
              id="licenseKeyError"
              class="form-error"
              style="color: var(--error); font-size: 0.85rem; margin-top: 0.25rem; display: none;"></div>
          </div>
          <div class="form-group">
            <label for="identifier" class="form-label"
              id="identifierLabel">Identifier</label>
            <input type="text" id="identifier" class="form-control"
              placeholder="Enter unique identifier" />
            <div
              id="identifierError"
              class="form-error"
              style="color: var(--error); font-size: 0.85rem; margin-top: 0.25rem; display: none;"></div>
          </div>
        </div>

        <div class="card">
          <div class="card-title">
            <span id="headersText">Headers</span>
            <button class="btn btn-outline btn-sm" id="addHeaderBtn">
              <i class="fas fa-plus"></i>
              <span id="addHeaderBtnText">Add Header</span>
            </button>
          </div>
          <div class="headers-container" id="headersContainer">
            <div class="header-row">
              <input type="text" class="form-control header-input"
                placeholder="Header name" value="Authorization" />
              <input type="text" class="form-control header-input"
                placeholder="Header value" value="Bearer <token>" />
              <button class="btn btn-outline btn-sm remove-header-btn">
                <i class="fas fa-times"></i>
              </button>
            </div>
          </div>
        </div>

        <div class="card">
          <div class="card-title">
            <span id="responseText">Response</span>
            <div class="response-actions"
              style="display: flex; gap: 0.5rem; align-items: center;">
              <button class="btn btn-outline btn-sm" id="testConnectionBtn"
                title="Test de conexión">
                <i class="fas fa-plug"></i>
              </button>
              <button class="btn btn-outline btn-sm" id="expandResponseBtn"
                title="Expandir/Colapsar">
                <i class="fas fa-expand"></i>
              </button>
              <button class="btn btn-outline btn-sm" id="copyResponseBtn"
                title="Copiar respuesta">
                <i class="far fa-copy"></i>
              </button>
              <button class="btn btn-outline btn-sm" id="clearResponseBtn">
                <i class="fas fa-trash-alt"></i>
                <span id="clearBtnText">Clear</span>
              </button>
              <button class="btn btn-primary btn-sm" id="sendRequestBtn">
                <i class="fas fa-paper-plane"></i>
                <span id="sendBtnText">Send Request</span>
              </button>
            </div>
          </div>
          <div class="response-container" id="responseContainer">
            <div id="responseStatus" class="status-indicator hidden"></div>
            <div class="response-tabs">
              <div class="response-tab active" data-tab="pretty"
                id="prettyTabText">Pretty</div>
              <div class="response-tab" data-tab="raw" id="rawTabText">Raw</div>
              <div class="response-tab" data-tab="headers"
                id="headersTabText">Headers</div>
            </div>
            <pre class="response-content"
              id="responsePretty"><code class="json"></code></pre>
            <div class="response-content hidden" id="responseRaw"></div>
            <div class="response-content hidden" id="responseHeaders"></div>
          </div>
        </div>
      </div>
    </div>

    <input type="file" id="importHistoryInput" accept="application/json"
      style="display:none" />

    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.8.0/highlight.min.js"></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.8.0/languages/json.min.js"></script>
    <script src="/js/playground.js"></script>
    <script>hljs.highlightAll();</script>
  </body>
</html>