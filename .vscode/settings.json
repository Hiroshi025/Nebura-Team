{
  "files.associations": {
    "*.ejs": "html",
    "*.ts": "typescript" // Asocia archivos .ts con TypeScript
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json, typedoc.json, tsconfig.build.tsbuildinfo, eslint.config.mjs, .prettierrc",
    ".eslintrc.cjs": ".eslintignore, .eslintrc.js, .eslintrc.json, .eslintrc.yaml, .eslintrc.yml, .editorconfig, jest.config.ts, .prettierrc, .lintstagedrc.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock, .gitattributes, .npmrc, .yarnrc, .pnpmfile.cjs, .npmignore, .yarnclean",
    "README.md": "CHANGELOG.md, CONTRIBUTING.md, LICENSE, .gitignore, .vscode/settings.json, .vscode/extensions.json, .vscode/tasks.json, CODE_OF_CONDUCT.md",
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/build": false,
    "**/documentation": true,
    "**/_test_": true,
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#1f6fd0",
    "activityBar.background": "#1f6fd0",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ee90bb",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#1f6fd0",
    "statusBar.background": "#1857a4",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#1f6fd0",
    "statusBarItem.remoteBackground": "#1857a4",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#1857a4",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#1857a499",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#1857a4",
  "symbols.folders.associations": {
    "views": "folder-interfaces",
    "locales": "folder-context",
    "schemas": "folder-models",
    "svg": "folder-svg",
  },
  "symbols.files.associations": {
    "*.controller.ts": "nest-controller",
    "*.service.ts": "nest-service",
    "*.guard.ts": "nest-guard",
    "*.middleware.ts": "nest-middleware",
  }
}
